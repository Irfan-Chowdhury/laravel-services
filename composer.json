{
    "name": "rschaaphuizen/laravel-services",
    "description": "Package to create services with artisan",
    "keywords":     ["laravel", "services", "artisan"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ruud Schaaphuizen",
            "email": "r.schaaphuizen@sqits.nl",
            "homepage": "https://www.sqits.nl",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/rschaaphuizen/laravel-services"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Rschaaphuizen\\Services\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rschaaphuizen\\Services\\ServicesServiceProvider"
            ]
        }
    }
}
